{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
   
    "add-item-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/addItem/AddItemWebPart.js",
          "manifest": "./src/webparts/addItem/AddItemWebPart.manifest.json"
        }
      ]
    },
    "subscribe-to-items-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/subscribeToItems/SubscribeToItemsWebPart.js",
          "manifest": "./src/webparts/subscribeToItems/SubscribeToItemsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
   
    "AddItemWebPartStrings": "lib/webparts/addItem/loc/{locale}.js",
    "SubscribeToItemsWebPartStrings": "lib/webparts/subscribeToItems/loc/{locale}.js"
  }
}
